node {
      def app
      stage('Clone repository') {
            checkout scm
      }
      stage('Build image') {

            app = docker.build("antonyngayo5028/python-uploader")
      }
       stage('Push image') {
            docker.withRegistry('https://registry.hub.docker.com', 'docker-registry') {
                app.push("${env.BUILD_NUMBER}")
                app.push("latest")
            }
      }